{
   "version": 2,
   "name": "StaticDelivr",
   "builds": [
      { "src": "server.js", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/img/(.*)", "dest": "/static/img/(.*)" },
      { "src": "/css/(.*)", "dest": "/static/css/(.*)" },
      { "src": "/js/(.*)", "dest": "/static/js/(.*)" },
      { "src": "/(.*)", "dest": "/server.js" }
   ]
}
