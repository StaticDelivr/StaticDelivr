{
   "version": 2,
   "name": "StaticDelivr",
   "builds": [
      { "src": "server.js", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/img/(.*)", "dest": "/static/img/$1" },
      { "src": "/css/(.*)", "dest": "/static/css/$1" },
      { "src": "/js/(.*)", "dest": "/static/js/$1" },
      { "src": "/(.*)", "dest": "/server.js" }
   ]
}
